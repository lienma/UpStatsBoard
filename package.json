{
  "name": "UpsBoard",
  "description": "Usenet + Plex + Stats Board, allows you to monitor your usenet services (SABnzbd+, Sonarr, Sick Beard, & CouchPotato), Plex Media Server, & your servers/computers (CPU Usage, Memory Usage, Bandwidth Usage, & Disk Space Usage).",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "build": "grunt build",
    "clean": "rimraf build"
  },
  "browser": {
    "backbone": "./bower_components/backbone/backbone.js",
    "bootstrap": "./bower_components/bootstrap/dist/js/bootstrap.js",
    "jquery": "./bower_components/jquery/dist/jquery.js",
    "jquery-ui": "./source/vendors/jquery-ui/jquery-ui-custom-1.11.4.js",
    "jqnotifybar": "./bower_components/jqnotifybar/jquery.notifyBar.js",
    "numeral": "./bower_components/numeral/numeral.js",
    "underscore": "./bower_components/underscore/underscore.js"
  },
  "browser-css": {
    "bootstrap": "./bower_components/bootstrap/dist/css/bootstrap.css",
    "bootstrap-theme": "./bower_components/bootstrap/dist/css/bootstrap-theme.css",
    "font-awesome": "./bower_components/font-awesome/css/font-awesome.css",
    "jquery-ui": "./source/vendors/jquery-ui/jquery-ui-custom.css",
    "jqnotifybar": "./bower_components/jqnotifybar/css/jquery.notifyBar.css"
  },
  "browser-font": {
    "bootstrap": "./bower_components/bootstrap/fonts/",
    "font-awesome": "./bower_components/font-awesome/fonts/"
  },
  "browserify": {
    "transform": [
      [
        "jadeify",
        {
          "compileDebug": false,
          "pretty": false
        }
      ],
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "jquery": "$",
    "jquery-ui": {
      "exports": "jqueryui",
      "depends": [
        "jquery:$"
      ]
    },
    "jqnotifybar": {
      "depends": [
        "jquery:$"
      ]
    },
    "bootstrap": {
      "exports": "bootstrap",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "dependencies": {
    "backbone": "^1.2.3",
    "bcrypt-nodejs": "0.0.3",
    "bcryptjs": "^2.3.0",
    "bluebird": "^3.0.2",
    "body-parser": "^1.14.1",
    "bookshelf": "^0.8.2",
    "bootstrap": "^3.3.6",
    "chalk": "^1.1.1",
    "colors": "1.1.*",
    "cookie-parser": "^1.4.0",
    "cookie-session": "^1.2.0",
    "csurf": "^1.8.3",
    "errorhandler": "^1.4.2",
    "express": "4.0.*",
    "express-favicon": "^1.0.1",
    "express-minify": "^0.1.6",
    "include-all": "^0.1.6",
    "ini": "^1.3.4",
    "jade": "^1.11.0",
    "jquery": "^2.2.0",
    "knex": "^0.8.6",
    "lodash": "^3.10.1",
    "method-override": "^2.3.5",
    "moment": "2.10.*",
    "morgan": "^1.6.1",
    "nib": "^1.1.0",
    "node-uuid": "^1.4.3",
    "passport": "^0.3.0",
    "request": "^2.65.0",
    "response-time": "^2.3.1",
    "serve-static": "^1.10.0",
    "socket.io": "^1.3.7",
    "ssh2": "^0.4.12",
    "stylus": "^0.52.4",
    "underscore": "^1.8.3",
    "winston": "^2.1.0",
    "xml2js": "^0.4.13"
  },
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-preset-es2015": "6.1.x",
    "babelify": "7.2.x",
    "browserify": "^13.0.0",
    "browserify-shim": "^3.8.12",
    "del": "^2.2.0",
    "gulp": "^3.9.0",
    "gulp-concat-css": "^2.2.0",
    "gulp-connect": "^2.3.1",
    "gulp-eslint": "^1.1.1",
    "gulp-livereload": "^3.8.1",
    "gulp-minify-css": "^1.2.3",
    "gulp-util": "^3.0.7",
    "jadeify": "^4.6.0",
    "uglifyify": "^3.0.1",
    "vinyl-source-stream": "^1.1.0"
  }
}
