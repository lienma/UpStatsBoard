extends ../layout

block stylesheet
  link(rel='stylesheet', href=locals.webRoot + '/stylesheets/setup.css?' + locals.seed)

block script
  script(type='text/javascript') Config.uuid = '#{locals.uuid}';
  script(type='text/javascript') require(['javascripts/config'], function(config) { require(['setup'], function(setup) { new setup(); }); });

block content
  .container#SetupWizard
    .row
      .h1 UpsBoard Setup Guide

    .row
      .panel.panel-default.wizard
        .panel-heading
          h4.panel-title Welcome to the setup guide for the UpsBoard App
        div
          .btn-group.btn-group-justified.wizard-tabs(role='group', aria-label='Navigation Bar')

        .panel-body
          div(style='display:none')#setupWelcome
          div(style='display:none')#setupApp
            form.form-horizontal
              .row
                .col-md-12
                  h4.step-title Application Server Settings
                  .form-group
                    label(for='appBindAddress').col-sm-2.control-label Bind Address
                    .col-sm-8
                      input(type='text', placeholder='Bind Address', value='#{locals.host}', aria-describedby='appBindAddressHelp', size=40, data-model-field='appBindAddress', data-model-validate='BindAddress').form-control#appBindAddress
                      span.help-block#appBindAddressHelp Host UpsBoard should listen on. IPv4 Address only.
                  .form-group
                    label(for='appPortNumber').col-sm-2.control-label Port Number
                    .col-sm-8
                      input(type='number', placeholder='Port Number', value='#{locals.port}', aria-describedby='appPortNumberHelp', size=5, data-model-field='appPortNumber', data-model-validate='PortNumber').form-control#appPortNumber
                      span.help-block#appPortNumberHelp Port UpsBoard should listen on. 
                        small#appPortNumberHelpNote Note: Having port number 1024 or lower will require root account to run.
                  .form-group
                    label(for='appWebRoot').col-sm-2.control-label Web Root
                    .col-sm-8
                      input(type='text', placeholder='Web Root', value='#{locals.webRoot}', aria-describedby='appWebRootHelp', size=40, data-model-field='appWebRoot', data-model-validate='WebRoot').form-control#appWebRoot
                      span.help-block#appWebRootHelp Used with reverse proxy. Leave empty for default.
                  .form-group
                    label(for='appLogHttpRequests').col-sm-2.control-label Log http Requests
                    .col-sm-8.checkbox
                      label
                        input(type='checkbox', checked, aria-describedby='appLogHttpRequestsHelp', data-model-field='appLogRequests')#appLogHttpRequests
                        span.help-block.help-block-checkbox Enabled
                      span.help-block#appLogHttpRequestsHelp Diabled if you don't want to fill your log files up with http requests.
                   .form-group
                    label(for='appCheckForUpdates').col-sm-2.control-label Check for Updates
                    .col-sm-8.checkbox
                      label
                        input(type='checkbox', checked, aria-describedby='appCheckForUpdatesHelp', data-model-field='appCheckForUpdates')#appCheckForUpdates
                        span.help-block.help-block-checkbox Enabled
                      span.help-block#appCheckForUpdatesHelp Diabled if you don't want the app to check for new updates.
                   .form-group
                    label(for='appAutoUpdate').col-sm-2.control-label Automatically Update
                    .col-sm-8.checkbox
                      label
                        input(type='checkbox', checked, aria-describedby='appAutoUpdateHelp', data-model-field='appAutoUpdate')#appAutoUpdate
                        span.help-block.help-block-checkbox Enabled
                      span.help-block#appAutoUpdateHelp Diabled if you don't want the app to automatically update itself.

              .row
                .col-md-12
                  h4.step-title Create Administrator Account
                  .form-group
                    label(for='adminEmail').col-sm-2.control-label Email address
                    .col-sm-8
                      input(type='email', placeholder='Email', aria-describedby='appAdminEmail', data-model-field='adminEmail', data-model-validate='Email').form-control#adminEmail
                      span.help-block#appAdminEmail The email addess of the administrator account.
                  .form-group
                    label(for='adminUsername').col-sm-2.control-label Username
                    .col-sm-8
                      input(type='text', placeholder='Username', aria-describedby='appAdminUsername', data-model-field='adminUsername', data-model-validate='Username').form-control#adminUsername
                      span.help-block#appAdminUsername The username used for the administrator account (at least 3 characters). 
                  .form-group
                    label(for='adminPassword').col-sm-2.control-label Password
                    .col-sm-8
                      input(type='password', placeholder='Password', aria-describedby='appAdminPassword').form-control#adminPassword
                      span.help-block#appAdminPassword Password of the administrator account (at lease 6 characters).
                  .form-group
                    label(for='adminPasswordConfirm').col-sm-2.control-label Confirm Password
                    .col-sm-8
                      input(type='password', placeholder='Confirm Password', aria-describedby='appAdminPasswordConfirm').form-control#adminPasswordConfirm
                      span.help-block#appAdminPasswordConfirm Please confirm the administrator account password.
              .row
                .col-md-12
                  .row-continue
                    button(role='button').btn.btn-info.btn-previous-page Previous
                    .pull-right
                      div.row-btn-inline
                        button(role='button', type='submit').btn.btn-primary.btn-next-page Next

          div(style='display:none')#setupUsenet
            ul.nav.nav-tabs.wizard-sub-tabs(role='tablist')
              .wizard-sub-title Supported Usenet Applications:
              li(role='presentation').active
                a(href='#usenetDownloaderApp', aria-controls='usenetDownloaderApp', role='tab', data-toggle='tab')#usenetDownloaderAppTab
                  .icos.icos-16.icos-usenet-app
                  span.wizard-sub-tabs-title Downloader App
                  i.icos.icos-16.glyphicon.icos-usenet-app-status
              li(role='presentation')
                a(href='#usenetTVShowApp', aria-controls='usenetTVShowApp', role='tab', data-toggle='tab')#usenetTVShowAppTab
                  .icos.icos-16.icos-usenet-app
                  span.wizard-sub-tabs-title TV Show App
                  i.icos.icos-16.glyphicon.icos-usenet-app-status
              li(role='presentation')
                a(href='#usenetMovieApp', aria-controls='usenetMovieApp', role='tab', data-toggle='tab')#usenetMovieAppTab
                  .icos.icos-16.icos-usenet-app
                  span.wizard-sub-tabs-title Movie App
                  i.icos.icos-16.glyphicon.icos-usenet-app-status
              li(role='presentation')
                a(href='#usenetMusicApp', aria-controls='usenetMusicApp', role='tab', data-toggle='tab')#usenetMusicAppTab
                  .icos.icos-16.icos-usenet-app
                  span.wizard-sub-tabs-title Music App
                  i.icos.icos-16.glyphicon.icos-usenet-app-status

            .tab-content.wizard-sub-content
              .tab-pane.fade.in.active(role='tabpanel')#usenetDownloaderApp
                .row
                  .col-md-12
                    h4.step-title Please Select Downloader App To Use
                .row
                  .col-md-12.col-sm-12.wizard-select-services
                    .col-md-6.col-sm-6.col-xs-6.wizard-select-service.wizard-select-service-left
                      a(data-service='sabnzbd')#usenetSabnzbdAppIcon
                        span.icos.icos-72.icos-sabnzbd
                    .col-md-6.col-sm-6.col-xs-6.wizard-select-service.wizard-select-service-right
                      a(data-service='nzbget')#usenetNzbgetAppIcon
                        span.icos.icos-72.icos-nzbget

                form(style='display:none;').form-horizontal#usenetSabnzbdApp
                  .row
                    .col-md-12
                      h4.step-title SABnzbd+ Server Settings
                      .form-group
                        label(for='sabnzbdEnable').col-sm-2.control-label Enable SABnzbd+
                        .col-sm-8.checkbox
                          label
                            input(type='checkbox').checkboxEnable.service-enable#sabnzbdEnable
                            |  Enabled
                      .form-group
                        label(for='sabnzbdHost').col-sm-2.control-label IP/Host Address
                        .col-sm-8
                          input(type='text', placeholder='IP/Host Address', aria-describedby='sabnzbdHostHelp', size=40, disabled).form-control.service-host#sabnzbdHost
                          span.help-block#sabnzbdHostHelp The IP Address or Host Address of the SABnzbd+ server.
                      .form-group
                        label(for='sabnzbdPortNumber').col-sm-2.control-label Port Number
                        .col-sm-8
                          input(type='number', placeholder='Port Number', aria-describedby='sabnzbdPortNumberHelp', size=5, disabled).form-control.service-port#sabnzbdPortNumber
                          span.help-block#sabnzbdPortNumberHelp Port number that SABnzbd+ is running on.
                            small Note: Default port number for SABnzbd is 8085.
                      .form-group
                        label(for='sabnzbdWebRoot').col-sm-2.control-label Web Root 
                          small (Optional)
                        .col-sm-8
                          input(type='text', placeholder='Web Root', aria-describedby='sabnzbdWebRootHelp', disabled).form-control.service-web-root#sabnzbdWebRoot
                          span.help-block#sabnzbdWebRootHelp This is used if your SABnzbd+ server is running behind a proxy and/or isn't on the root location "/".
                      .form-group
                        label(for='sabnzbdApiKey').col-sm-2.control-label API Key 
                        .col-sm-8
                          input(type='text', placeholder='API Key', aria-describedby='sabnzbdApiKeyHelp', disabled).form-control.service-api-key#sabnzbdApiKey
                          span.help-block#sabnzbdApiKeyHelp The API key can be found in the SABnzbd+ Settings > General.
                      .form-group
                        label(for='sabnzbdSSLEnable').col-sm-2.control-label Use SSL/HTTPS
                        .col-sm-8.checkbox
                          label
                            input(type='checkbox', aria-describedby='sabnzbdSSLEnableHelp', disabled).checkboxEnable.service-use-ssl#sabnzbdSSLEnable
                            span.help-block.help-block-checkbox Enabled
                          span.help-block#sabnzbdSSLEnableHelp Use if you have SSL enabled on your SABnzbd+ server.

                form(style='display:none;').form-horizontal#usenetNzbgetApp
                  .row
                    .col-md-12
                      h4.step-title NZBGet Server Settings

              .tab-pane.fade(role='tabpanel')#usenetTVShowApp
                .row
                  .col-md-12
                    h4.step-title Please Select TV Show App To Use
                .row
                  .col-md-12.col-sm-12.wizard-select-services
                    .col-md-6.col-sm-6.wizard-select-service.wizard-select-service-left
                      a(data-service='sonarr')#usenetSonarrAppIcon
                        span.icos.icos-72.icos-sonarr
                    .col-md-6.col-sm-6.wizard-select-service.wizard-select-service-right
                      a(data-service='sickbeard')#usenetSickbeardAppIcon
                        span.icos.icos-72.icos-sickbeard

                form(style='display:none;').form-horizontal#usenetSonarrPage
                  .row
                    .col-md-12
                      h4.step-title Sonarr Server Settings
                      .form-group
                        label(for='sonarrEnable').col-sm-2.control-label Enable Sonarr
                        .col-sm-9.checkbox
                          label
                            input(type='checkbox').checkboxEnable.service-enable#sonarrEnable
                            |  Enabled
                      .form-group
                        label(for='sonarrHost').col-sm-2.control-label IP/Host Address
                        .col-sm-8
                          input(type='text', placeholder='IP/Host Address', aria-describedby='sonarrHostHelp', size=40, disabled).form-control.service-host#sonarrHost
                          span.help-block#sonarrHostHelp The IP Address or Host Address of the Sonarr server.
                      .form-group
                        label(for='sonarrPortNumber').col-sm-2.control-label Port Number
                        .col-sm-8
                          input(type='number', placeholder='Port Number', aria-describedby='sonarrPortNumberHelp', size=5, disabled).form-control.service-port#sonarrPortNumber
                          span.help-block#sonarrPortNumberHelp Port number that Sonarr is running on.
                            small Note: Default port number for Sonarr is 8989.
                      .form-group
                        label(for='sonarrWebRoot').col-sm-2.control-label Web Root 
                          small (Optional)
                        .col-sm-8
                          input(type='text', placeholder='Web Root', aria-describedby='sonarrWebRootHelp', disabled).form-control.service-web-root#sonarrWebRoot
                          span.help-block#sonarrWebRootHelp This is used if your Sonarr server is running behind a proxy and/or isn't on the root location "/".
                      .form-group
                        label(for='sonarrApiKey').col-sm-2.control-label API Key 
                        .col-sm-8
                          input(type='text', placeholder='API Key', aria-describedby='sonarrApiKeyHelp', disabled).form-control.service-api-key#sonarrApiKey
                          span.help-block#sonarrApiKeyHelp The API key can be found in the Sonarr Settings > General.
                      .form-group
                        label(for='sonarrSSLEnable').col-sm-2.control-label Use SSL/HTTPS
                        .col-sm-8.checkbox
                          label
                            input(type='checkbox', aria-describedby='sonarrSSLEnableHelp', disabled).checkboxEnable.service-use-ssl#sonarrSSLEnable
                            span.help-block.help-block-checkbox Enabled
                          span.help-block#sonarrSSLEnableHelp Use if you have SSL enabled on your Sonarr server.

                form(style='display:none;').form-horizontal#usenetSickbeardPage
                  .row
                    .col-md-12
                      h4.step-title Sick Beard Server Settings
                      .form-group
                        label(for='sickbeardEnable').col-sm-2.control-label Enable Sick Beard
                        .col-sm-9.checkbox
                          label
                            input(type='checkbox').checkboxEnable.service-enable#sickbeardEnable
                            |  Enabled
                      .form-group
                        label(for='sickbeardHost').col-sm-2.control-label IP/Host Address
                        .col-sm-8
                          input(type='text', placeholder='IP/Host Address', aria-describedby='sickbeardHostHelp', size=40, disabled).form-control.service-host#sickbeardHost
                          span.help-block#sickbeardHostHelp The IP Address or Host Address of the Sick Beard server.
                      .form-group
                        label(for='sickbeardPortNumber').col-sm-2.control-label Port Number
                        .col-sm-8
                          input(type='number', placeholder='Port Number', aria-describedby='sickbeardPortNumberHelp', size=5, disabled).form-control.service-port#sickbeardPortNumber
                          span.help-block#sickbeardPortNumberHelp Port number that Sick Beard is running on.
                            small Note: Default port number for Sonarr is 8081.
                      .form-group
                        label(for='sickbeardWebRoot').col-sm-2.control-label Web Root 
                          small (Optional)
                        .col-sm-8
                          input(type='text', placeholder='Web Root', aria-describedby='sickbeardWebRootHelp', disabled).form-control.service-web-root#sickbeardWebRoot
                          span.help-block#sickbeardWebRootHelp This is used if your Sick Beard server is running behind a proxy and/or isn't on the root location "/".
                      .form-group
                        label(for='sickbeardApiKey').col-sm-2.control-label API Key 
                        .col-sm-8
                          input(type='text', placeholder='API Key', aria-describedby='sickbeardApiKeyHelp', disabled).form-control.service-api-key#sickbeardApiKey
                          span.help-block#sickbeardApiKeyHelp The API key can be found in the Sick Beard Settings > General.
                      .form-group
                        label(for='sickbeardSSLEnable').col-sm-2.control-label Use SSL/HTTPS
                        .col-sm-8.checkbox
                          label
                            input(type='checkbox', aria-describedby='sickbeardSSLEnableHelp', disabled).checkboxEnable.service-use-ssl#sickbeardSSLEnable
                            span.help-block.help-block-checkbox Enabled
                          span.help-block#sickbeardSSLEnableHelp Use if you have SSL enabled on your Sick Beard server.
                    .row
                    .col-md-12
                      .row-continue
                        button(role='button').btn.btn-info.btn-previous-page Previous
                        .pull-right
                          .service-test-msg.row-btn-inline
                          button(role='button', disabled).btn.btn-info.service-test Test Connection
                          div.row-btn-inline
                            button(role='button').btn.btn-primary.btn-next-page Next

              .tab-pane.fade(role='tabpanel')#usenetMovieApp
                form.form-horizontal
                  .row
                    .col-md-12
                      h4.step-title CouchPotato Server Settings
                      .form-group
                        label(for='couchpotatoEnable').col-sm-2.control-label Enable CouchPotato
                        .col-sm-9.checkbox
                          label
                            input(type='checkbox').checkboxEnable.service-enable#couchpotatoEnable
                            |  Enabled
                      .form-group
                        label(for='couchpotatoHost').col-sm-2.control-label IP/Host Address
                        .col-sm-8
                          input(type='text', placeholder='IP/Host Address', aria-describedby='couchpotatoHostHelp', size=40, disabled).form-control.service-host#couchpotatoHost
                          span.help-block#couchpotatoHostHelp The IP Address or Host Address of the CouchPotato server.
                      .form-group
                        label(for='couchpotatoPortNumber').col-sm-2.control-label Port Number
                        .col-sm-8
                          input(type='number', placeholder='Port Number', aria-describedby='couchpotatoPortNumberHelp', size=5, disabled).form-control.service-port#couchpotatoPortNumber
                          span.help-block#couchpotatoPortNumberHelp Port number that CouchPotato is running on.
                            small Note: Default port number for CouchPotato is 5050.
                      .form-group
                        label(for='couchpotatoWebRoot').col-sm-2.control-label Web Root 
                          small (Optional)
                        .col-sm-8
                          input(type='text', placeholder='Web Root', aria-describedby='couchpotatoWebRootHelp', disabled).form-control.service-web-root#couchpotatoWebRoot
                          span.help-block#couchpotatoWebRootHelp This is used if your CouchPotato server is running behind a proxy and/or isn't on the root location "/".
                      .form-group
                        label(for='couchpotatoApiKey').col-sm-2.control-label API Key 
                        .col-sm-8
                          input(type='text', placeholder='API Key', aria-describedby='couchpotatoApiKeyHelp', disabled).form-control.service-api-key#couchpotatoApiKey
                          span.help-block#couchpotatoApiKeyHelp The API key can be found in the CouchPotato Settings.
                      .form-group
                        label(for='couchpotatoSSLEnable').col-sm-2.control-label Use SSL/HTTPS
                        .col-sm-8.checkbox
                          label
                            input(type='checkbox', aria-describedby='couchpotatoSSLEnableHelp', disabled).checkboxEnable.service-use-ssl#couchpotatoSSLEnable
                            span.help-block.help-block-checkbox Enabled
                          span.help-block#couchpotatoSSLEnableHelp Use if you have SSL enabled on your CouchPotato server.

              .tab-pane.fade(role='tabpanel')#usenetMusicApp
                form.form-horizontal
                  .row
                    .col-md-12
                      h4.step-title Headphones Server Settings
                      .form-group
                        label(for='headphonesEnable').col-sm-2.control-label Enable Headphones
                        .col-sm-9.checkbox
                          label
                            input(type='checkbox').checkboxEnable.service-enable#headphonesEnable
                            |  Enabled
                      .form-group
                        label(for='headphonesHost').col-sm-2.control-label IP/Host Address
                        .col-sm-8
                          input(type='text', placeholder='IP/Host Address', aria-describedby='headphonesHostHelp', size=40, disabled).form-control.service-host#headphonesHost
                          span.help-block#headphonesHostHelp The IP Address or Host Address of the Headphones server.
                      .form-group
                        label(for='headphonesPortNumber').col-sm-2.control-label Port Number
                        .col-sm-8
                          input(type='number', placeholder='Port Number', aria-describedby='headphonesPortNumberHelp', size=5, disabled).form-control.service-port#headphonesPortNumber
                          span.help-block#headphonesPortNumberHelp Port number that Headphones is running on.
                            small Note: Default port number for Headphones is 8181.
                      .form-group
                        label(for='headphonesWebRoot').col-sm-2.control-label Web Root 
                          small (Optional)
                        .col-sm-8
                          input(type='text', placeholder='Web Root', aria-describedby='headphonesWebRootHelp', disabled).form-control.service-web-root#headphonesWebRoot
                          span.help-block#headphonesWebRootHelp This is used if your Headphones server is running behind a proxy and/or isn't on the root location "/".
                      .form-group
                        label(for='headphonesApiKey').col-sm-2.control-label API Key 
                        .col-sm-8
                          input(type='text', placeholder='API Key', aria-describedby='headphonesApiKeyHelp', disabled).form-control.service-api-key#headphonesApiKey
                          span.help-block#headphonesApiKeyHelp The API key can be found in the Headphones Settings.
                      .form-group
                        label(for='headphonesSSLEnable').col-sm-2.control-label Use SSL/HTTPS
                        .col-sm-8.checkbox
                          label
                            input(type='checkbox', aria-describedby='headphonesSSLEnableHelp', disabled).checkboxEnable.service-use-ssl#headphonesSSLEnable
                            span.help-block.help-block-checkbox Enabled
                          span.help-block#headphonesSSLEnableHelp Use if you have SSL enabled on your Headphones server.
            .row
              .col-md-12
                .row-continue
                  button(role='button').btn.btn-info.btn-previous-page Previous
                  .pull-right
                    .btn-app-test-msg.row-btn-inline
                    button(role='button', disabled, style='display:none').btn.btn-info.btn-app-test Test Connection
                    div.row-btn-inline
                      button(role='button', type='submit').btn.btn-primary.btn-next-page Next

          div(style='display:none')#setupPlex
            ul.nav.nav-tabs.wizard-sub-tabs(role='tablist')
              .wizard-sub-title Plex Apps:
              li(role='presentation').active
                a(href='#plexMediaServer', aria-controls='plexMediaServer', role='tab', data-toggle='tab')#plexMediaServerTab
                  .icos.icos-16.icos-plex
                  span.wizard-sub-tabs-title Plex Media Server
                  i.icos.icos-16.glyphicon.icos-usenet-app-status
              li(role='presentation')
                a(href='#plexPyApp', aria-controls='plexPyApp', role='tab', data-toggle='tab')#plexPyAppTab
                  .icos.icos-16.icos-plexpy
                  span.wizard-sub-tabs-title PlexPy
                  i.icos.icos-16.glyphicon.icos-usenet-app-status

            form.form-horizontal
              .tab-content.wizard-sub-content
                .tab-pane.fade.in.active(role='tabpanel')#plexMediaServer
                  .row
                    .col-md-12
                      h4.step-title Plex.tv Account Credentials
                      .form-group
                        label(for='plexAccountUsername').col-sm-2.control-label Username
                        .col-sm-8
                          input(type='text', placeholder='Username', aria-describedby='plexAccountUsernameHelp', data-model-field='plexUsername', data-model-validate='Username').form-control.plex-tv-account#plexAccountUsername
                          span.help-block#plexAccountUsernameHelp Your username for your Plex.tv account.
                      .form-group
                        label(for='plexAccountPassword').col-sm-2.control-label Password
                        .col-sm-8
                          input(type='password', placeholder='Password', aria-describedby='plexAccountPasswordHelp', data-model-field='plexPassword', data-model-validate='Password').form-control.plex-tv-account#plexAccountPassword
                          span.help-block#plexAccountPasswordHelp Your password for your Plex.tv account.
                      .form-group
                        .col-sm-8.col-sm-push-2
                          button(role='button', disabled, data-loading-text='Loading', autocomplete='off').btn.btn-default.btn-sm.btn-plex-tv-auth Authenicate with Plex.tv
                          span.btn-inline-msg.btn-plex-tv-auth-msg
                  .row
                    .col-md-12
                      h4.step-title Plex Media Server Settings
                      .form-group
                        label(for='plexMediaServerHost').col-sm-2.control-label IP/Host Address
                        .col-sm-8
                          input(type='text', placeholder='IP/Host Address', aria-describedby='plexMediaServerHostHelp', size=40, data-model-field='plexServerHost', data-model-validate='Host').form-control.plex-media-server#plexMediaServerHost
                          span.help-block#plexMediaServerHostHelp The IP Address or Host Address of the Plex Media Server.
                      .form-group
                        label(for='plexMediaServerPortNumber').col-sm-2.control-label Port Number
                        .col-sm-8
                          input(type='number', placeholder='Port Number', aria-describedby='plexMediaServerPortNumberHelp', value=32400, size=5, data-model-field='plexServerPort', data-model-validate='Port').form-control.plex-media-server#plexMediaServerPortNumber
                          span.help-block#plexMediaServerPortNumberHelp Port number that Plex Media Server is running on.
                            small Note: Default port number for Plex Media Server is 32400.
                      .form-group
                        label(for='plexMediaServerSSLEnable').col-sm-2.control-label Use SSL/HTTPS
                        .col-sm-8.checkbox
                          label
                            input(type='checkbox', aria-describedby='plexMediaServerSSLEnableHelp', data-model-field='plexServerUseSSL').checkboxEnable#plexMediaServerSSLEnable
                            span.help-block.help-block-checkbox Enabled
                          span.help-block#plexMediaServerSSLEnableHelp Use if you have SSL enabled on your Plex Media Server.
                      .form-group
                        .col-sm-8.col-sm-push-2
                          button(role='button', disabled).btn.btn-default.btn-sm.btn-plex-text-conn Test Connection
                          span.btn-inline-msg.btn-plex-text-conn-msg

                      h4.step-title Other Plex Settings
                      .form-group
                        label(for='plexDefaultMovie').col-sm-2.control-label Default Movie Library ID
                        .col-sm-8
                          input(type='number', placeholder='Default Movie Library ID', aria-describedby='plexDefaultMovieHelp', size=5, data-model-field='plexDefaultMovie', data-model-validate='DefaultMovie').form-control#plexDefaultMovie
                          span.help-block#plexDefaultMovieHelp The ID of your default movie library. 
                            button(role='button', data-type='movie', data-field='plexDefaultMovie', disabled).btn.btn-default.btn-xs.btn-plex-pick-library.btn-plex-pick-movie Click here to pick library.
                      .form-group
                        label(for='plexDefaultTVShow').col-sm-2.control-label Default TV Show Library ID
                        .col-sm-8
                          input(type='number', placeholder='Default TV Show Library ID', aria-describedby='plexDefaultTVShowHelp', size=5, data-model-field='plexDefaultTVShow', data-model-validate='DefaultTVShow').form-control#plexDefaultTVShow
